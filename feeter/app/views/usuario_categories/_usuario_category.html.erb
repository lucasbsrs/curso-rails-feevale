<% index = "#{Time.now.to_i}#{Time.now.nsec}" %>
<%= fields_for "category[usuario_categories_attributes][#{index}]",
  usuario_category do |f|%>
<tr>
  <td>
    <%=  usuario_category.usuario.try(:nome) %>
  </td>
  <td>
    <%= f.text_field :papel %>
  </td>
  <td>
    <a href="#" type="button" class="btn btn-default btn-primary remove-usuario-category">
      <span class="glyphicon glyphicon-minus" aria-hidden="true"></span>
    </a>
      <%= f.hidden_field :id %>
      <%= f.hidden_field :usuario_id %>
      <%= f.hidden_field :papel %>
      <%= f.hidden_field :_destroy, :class => :destroy_record %>
  </td>
</tr>
<% end %>
