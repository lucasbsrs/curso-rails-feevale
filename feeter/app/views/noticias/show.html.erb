<div class="page-header">
  <%= link_to noticias_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_noticia_path(@noticia), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show noticia</h1>
</div>

<dl class="dl-horizontal">
  <dt>Titulo:</dt>
  <dd><%= @noticia.titulo %></dd>

  <dt>Conteudo:</dt>
  <dd><%= @noticia.conteudo %></dd>

  <dt>Autor:</dt>
  <dd><%= @noticia.autor %></dd>

  <dt>Tags:</dt>
  <dd><%= @noticia.tags %></dd>

</dl>
